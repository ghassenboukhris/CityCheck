<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CityCheck.Signup">
  <StackLayout>
    <Entry
      x:Name="Username"
      Placeholder="Nom"
  />
    <Entry
      x:Name="Name"
    Placeholder="Prenom"
  />
    <Entry
      x:Name="Email"
 Placeholder="Email"
  />
    <Entry
      x:Name="Password"
 Placeholder="Mot de passe"
  />
    <Button
      Clicked="Signuppp"
      Text="Signupp"
      />
  </StackLayout>
</ContentPage>